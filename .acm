# Create template (if not exists) and Open file.
function t {
  if [ ! -f $1.cc ]; then
    echo "Creating: $1.cc"
    cp ~/acm/template.cc $1.cc
  fi
  v $1.cc
}


alias ta="t a"
alias tb="t b"
alias tc="t c"
alias td="t d"
alias te="t e"
alias tf="t f"
alias tg="t g"
